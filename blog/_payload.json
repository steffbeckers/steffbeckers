b74a`}\n}\n\n$StoreScope = 'LocalMachine'\n$StoreName = 'root'\n\n$Store = New-Object  -TypeName System.Security.Cryptography.X509Certificates.X509Store  -ArgumentList $StoreName, $StoreScope\n$Store.Open([System.Security.Cryptography.X509Certificates.OpenFlags]::ReadWrite)\n$Store.Add($cert)\n\n$Store.Close()\n",[354],{"type":24,"tag":196,"props":355,"children":356},{"__ignoreMap":8},[357,365,374,384,393,402,411,419,427,436,444,452,461,470,478,487,496,505,513],{"type":24,"tag":200,"props":358,"children":359},{"class":202,"line":203},[360],{"type":24,"tag":200,"props":361,"children":362},{},[363],{"type":30,"value":364},"$cert = New-SelfSignedCertificate -DnsName \"localhost\", \"localhost\" -CertStoreLocation \"cert:\\LocalMachine\\My\" -NotAfter (Get-Date).AddYears(5)\n",{"type":24,"tag":200,"props":366,"children":368},{"class":202,"line":367},2,[369],{"type":24,"tag":200,"props":370,"children":371},{},[372],{"type":30,"value":373},"$thumb = $cert.GetCertHashString()\n",{"type":24,"tag":200,"props":375,"children":377},{"class":202,"line":376},3,[378],{"type":24,"tag":200,"props":379,"children":381},{"emptyLinePlaceholder":380},true,[382],{"type":30,"value":383},"\n",{"type":24,"tag":200,"props":385,"children":387},{"class":202,"line":386},4,[388],{"type":24,"tag":200,"props":389,"children":390},{},[391],{"type":30,"value":392},"For ($i=44300; $i -le 44399; $i++) {\n",{"type":24,"tag":200,"props":394,"children":396},{"class":202,"line":395},5,[397],{"type":24,"tag":200,"props":398,"children":399},{},[400],{"type":30,"value":401},"    netsh http delete sslcert ipport=0.0.0.0:$i\n",{"type":24,"tag":200,"props":403,"children":405},{"class":202,"line":404},6,[406],{"type":24,"tag":200,"props":407,"children":408},{},[409],{"type":30,"value":410},"}\n",{"type":24,"tag":200,"props":412,"children":414},{"class":202,"line":413},7,[415],{"type":24,"tag":200,"props":416,"children":417},{"emptyLinePlaceholder":380},[418],{"type":30,"value":383},{"type":24,"tag":200,"props":420